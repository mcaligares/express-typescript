{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "Express API Template Project",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "lint": "eslint . --ext .ts",
    "test": "DOTENV_CONFIG_PATH=.env.test jest --verbose --setupFiles dotenv/config",
    "test:coverage": "npm run test -- --detectOpenHandles --silent --coverage",
    "test:dev": "npm run test -- --watchAll",
    "sequelize": "sequelize"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:coverage && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^9.0.1",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.20.19",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.13",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.10.2",
    "typescript": "^4.5.4"
  },
  "_moduleAliases": {
    "@/endpoints": "src/endpoints",
    "@/models": "src/models",
    "@/middlewares": "src/middlewares",
    "@/repositories": "src/repositories",
    "@/routes": "src/routes",
    "@/services": "src/services",
    "@/utils": "src/utils"
  }
}