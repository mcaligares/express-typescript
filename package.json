{
  "name": "express-typescript",
  "version": "0.0.1",
  "description": "Express project with TypeScript support ready to start coding",
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=development node -r ts-node/register ./src/index.ts",
    "dev": "NODE_ENV=development nodemon",
    "clean": "rm -rf ./dist",
    "build": "NODE_ENV=production npm run lint && npm run clean && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "NODE_ENV=test jest",
    "test-watch": "NODE_ENV=test jest --watch",
    "test-coverage": "NODE_ENV=test jest --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "github.com:mcaligares/express-typescript.git"
  },
  "keywords": [
    "express",
    "node",
    "typescript"
  ],
  "author": "mcaligares@gmail.com",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "mongoose": "^5.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.7",
    "@types/supertest": "^2.0.7",
    "jest": "^24.8.0",
    "nodemon": "^2.0.13",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^4.3.5"
  }
}
