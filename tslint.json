{
  "extends": "tslint:recommended",
  "rules": {
    "curly": false,
    "semicolon": false,
    "ban-types": false,
    "no-console": false,
    "member-access": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "member-ordering": [true, {"order": "instance-sandwich"}],
    "quotemark": [true, "single"]
  }
}